{
  "name": "hyppos-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.2.2",
    "antd": "4.6.1",
    "classnames": "2.2.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@craco/craco": "5.6.4",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "@types/react-router": "5.1.7",
    "@types/react-router-dom": "5.1.5",
    "craco-less": "1.15.0",
    "eslint": "6.8.0",
    "node-sass": "4.14.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.8",
    "react-scripts": "3.4.3",
    "typescript": "3.9.5"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --coverage",
    "test-ci": "CI=true craco test",
    "lint": "eslint src --ext .jsx,.js,.tsx,.ts --max-warnings=0",
    "security-check": "yarn audit --level moderate --groups dependencies; [[ $? -ge 4 ]] && exit 1 || exit 0",
    "ci-check": "yarn run test-ci && yarn run lint && yarn run security-check"
  },
  "engines": {
    "node": "^10 || ^11 || ^12 || ^13 || ^14",
    "yarn": "^1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
